{
  "name": "federalist-uswds-gatsby",
  "description": "A Federalist template using USWDS 2.0 + Gatsby",
  "version": "1.0.0",
  "scripts": {
    "clean": "rimraf .cache public _site",
    "build": "gatsby build",
    "reset": "npx rimraf .git",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "federalist": "npm run federalist:build && npm run federalist:copy",
    "federalist:build": "gatsby build --prefix-paths",
    "federalist:copy": "mkdir -p _site && rm -rf _site/* && cp -r public/* _site/"
  },
  "dependencies": {
    "gatsby": "^4.13.0",
      "gatsby-awesome-pagination": "^0.3.8",
      "gatsby-plugin-manifest": "^4.13.0",
      "gatsby-plugin-netlify-cms": "^6.13.0",
      "gatsby-plugin-offline": "^5.13.0",
      "gatsby-plugin-react-helmet": "^5.13.0",
      "gatsby-plugin-robots-txt": "^1.7.1",
      "gatsby-plugin-sass": "^5.13.0",
      "gatsby-plugin-sitemap": "^5.13.0",
      "gatsby-source-filesystem": "^4.13.0",
      "gatsby-transformer-remark": "^5.13.0",
      "netlify-cms-app": "^2.15.72",
      "prop-types": "^15.8.1",
      "react": "^17.0.2",
      "react-dom": "^17.0.2",
      "react-helmet": "^6.1.0",
      "sass": "^1.51.0",
      "uswds": "^2.13.3"
  },
  "devDependencies": {
    "eslint-config-react-app": "^7.0.0",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2"
  },
  "engines": {
    "node": "16.x.x",
    "npm": "8.x.x"
  }
}

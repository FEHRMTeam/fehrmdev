{
  "name": "federalist-uswds-gatsby",
  "description": "A Federalist template using USWDS 2.0 + Gatsby",
  "version": "2.0.0",
  "scripts": {
    "clean": "rimraf .cache public _site",
    "build": "gatsby build",
    "reset": "npx rimraf .git",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "federalist": "npm run federalist:build && npm run federalist:copy",
    "federalist:build": "gatsby build --prefix-paths",
    "federalist:copy": "mkdir -p _site && rm -rf _site/* && cp -r public/* _site/"
  },
  "dependencies": {
    "@uswds/uswds": "^3.13.0",
    "decap-cms-app": "^3.6.4",
    "gatsby": "^5.15.0",
    "gatsby-awesome-pagination": "^0.3.8",
    "gatsby-plugin-decap-cms": "^4.0.4",
    "gatsby-plugin-manifest": "^5.15.0",
    "gatsby-plugin-offline": "^6.15.0",
    "gatsby-plugin-react-helmet": "^6.15.0",
    "gatsby-plugin-robots-txt": "^1.8.0",
    "gatsby-plugin-sass": "^6.15.0",
    "gatsby-plugin-sitemap": "^6.15.0",
    "gatsby-source-filesystem": "^5.15.0",
    "gatsby-transformer-remark": "^6.15.0",
    "nanoid": "^5.1.6",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet": "^6.1.0",
    "sass": "^1.93.0",
    "showdown": "^2.1.0"
  },
  "devDependencies": {
    "eslint-config-react-app": "^7.0.1",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1"
  },
  "engines": {
    "node": "20.x.x",
    "npm": "10.x.x"
  }
}
